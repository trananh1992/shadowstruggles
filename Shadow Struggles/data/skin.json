{
	com.badlogic.gdx.graphics.g2d.BitmapFont: {
		basic-font: { file: data/fonts/default.fnt }, 
		default-font: { file: data/fonts/font-large.fnt },
		small-font: { file: data/fonts/font-small.fnt } ,
		medium-font: { file: data/fonts/font-medium.fnt },
		andalus-font: { file: data/fonts/andalus.fnt },
		gg-font:{file: data/fonts/font-gg.fnt}
	},
	com.badlogic.gdx.graphics.Color: {
		green: { a: 1, b: 0, g: 1, r: 0 },
		white: { a: 1, b: 1, g: 1, r: 1 },
		red: { a: 1, b: 0, g: 0, r: 1 },
		black: { a: 1, b: 0, g: 0, r: 0 }
	},
	com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
        default: { down: button_down, up: button },
        toggle: { down: button_down, checked: button_down, up: button }
	},
	com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
        default: { down: button_down, up: button, font: default-font, fontColor: black },
        toggle: { down: button_down, up: button, checked: button_down, font: default-font, fontColor: white, downFontColor: red },
        blur: { down: button_blur, up: button_blur, font: andalus-font, fontColor: black },
        drawer: { down: drawer, up: drawer, font: andalus-font, fontColor: white }
	},
	com.badlogic.gdx.scenes.scene2d.ui.ScrollPane$ScrollPaneStyle: {
        default: { vScrollKnob: scroll_bar }
	},
	com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
        default: { font: andalus-font, fontColor: white }
	},
	com.badlogic.gdx.scenes.scene2d.ui.SelectBox$SelectBoxStyle: {
		default: { listBackground: rect, listSelection: rect, font: andalus-font, fontColor: black, background: selector }
	},
	com.badlogic.gdx.scenes.scene2d.ui.CheckBox$CheckBoxStyle: {
		default: { checkboxOn: checkbox-on, checkboxOff: checkbox, font: andalus-font, fontColor: white }
	},
	com.badlogic.gdx.scenes.scene2d.ui.Slider$SliderStyle: {
		default-horizontal: { background: slider, knob: slider-knob }
	},
	com.badlogic.gdx.scenes.scene2d.ui.Window$WindowStyle: {
		default: { titleFont: default-font, background: default-window, titleFontColor: white }
	}
}